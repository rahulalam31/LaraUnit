{
    "name": "rahulalam31/lara-unit",
    "description": "This is my package lara-unit",
    "keywords": [
        "rahulalam31",
        "lara-unit"
    ],
    "homepage": "https://github.com/rahulalam31/lara-unit",
    "license": "MIT",
    "authors": [
        {
            "name": "Rahul Alam",
            "email": "gaberialcramern31@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "friendsofphp/php-cs-fixer": "^3.21.1",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Rahulalam31\\LaraUnit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rahulalam31\\LaraUnit\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
